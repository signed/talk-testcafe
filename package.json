{
  "name": "contract-tests",
  "version": "1.0.0",
  "description": "slides for the contract tests talk",
  "main": "index.js",
  "scripts": {
    "testcafe:run": "testcafe chrome fixtures/**/*.ts",
    "testcafe:headless": "testcafe chrome:headless fixtures/**/*.ts --reporter spec,xunit:reports/report.xml,json:reports/report.json",
    "testcafe:debug": "testcafe $NODE_DEBUG_OPTION chrome",
    "testcafe:ci": "start-server-and-test start http://localhost:8080 testcafe:headless",
    "start": "webpack-dev-server --mode development",
    "start:dev": "yarn start --open",
    "present": "node scripts/scribeV2.js",
    "present:old": "yarn scribe && browser-sync start --server --no-online --no-ui --startPath out/presentation.html --watch --files out/presentation.html --watchEvents add",
    "scribe": "node scripts/scribe.js"
  },
  "private": true,
  "dependencies": {
    "asciidoctor-reveal.js": "2.0.0",
    "asciidoctor.js": "1.5.9",
    "fs-extra": "8.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@types/node": "12.6.2",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "awesome-typescript-loader": "5.2.1",
    "browser-sync": "2.26.7",
    "chokidar-cli": "1.2.2",
    "html-webpack-plugin": "3.2.0",
    "opener": "1.5.1",
    "start-server-and-test": "1.9.1",
    "testcafe": "1.3.2",
    "testcafe-react-selectors": "3.1.0",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.5.3",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  }
}
