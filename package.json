{
  "name": "@signed/talk-testcafe",
  "version": "1.0.0",
  "private": true,
  "description": "slides and code for the testcafe introduction",
  "main": "index.js",
  "scripts": {
    "testcafe:run": "testcafe chrome",
    "testcafe:run:selenoid": "SELENIUM_CAPABILITIES=selenoid/capabilities.json testcafe selenium:chrome",
    "testcafe:headless": "testcafe chrome:headless --reporter spec,xunit:reports/report.xml,json:reports/report.json",
    "testcafe:quarantine": "testcafe --reporter spec,raw:raw.log --quarantine-mode attemptLimit=4,successThreshold=2 chrome fixtures/quarantine/quarantine.fixture.ts",
    "testcafe:debug": "testcafe $NODE_DEBUG_OPTION chrome",
    "testcafe:watch": "testcafe --live chrome",
    "testcafe:ci": "start-server-and-test start http://localhost:8080 testcafe:headless",
    "start": "webpack server --mode development",
    "start:dev": "yarn start --open",
    "typecheck": "tsc --noEmit",
    "deploy": "gh-pages -d dist",
    "build": "webpack --mode=production",
    "present": "scribe",
    "lint": "eslint src fixtures"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@signed/prettier-config": "0.1.0",
    "@signed/scribe": "0.4.0",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "8.23.1",
    "gh-pages": "4.0.0",
    "html-webpack-plugin": "5.5.0",
    "prettier": "2.7.1",
    "start-server-and-test": "1.14.0",
    "testcafe": "2.0.0",
    "testcafe-browser-provider-selenium": "1.2.0",
    "testcafe-react-selectors": "5.0.2",
    "testcafe-reporter-raw": "npm:@signed/testcafe-reporter-raw@0.1.1",
    "ts-loader": "9.3.1",
    "typescript": "4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.0"
  },
  "prettier": "@signed/prettier-config"
}
